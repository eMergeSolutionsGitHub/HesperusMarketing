<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#EBEBEB">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/relativeLayout7"
        android:id="@+id/relativeLayoutquentity"
        android:visibility="visible"
        android:background="#4d4d4d"
        android:layout_marginLeft="20dp"
        android:layout_alignRight="@+id/relativeLayout5"
        android:layout_alignEnd="@+id/relativeLayout5"
        android:layout_marginTop="20dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Shelf"
            android:id="@+id/textView20"
            android:textColor="#ffffff"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="5dp" />

        <EditText
            android:layout_width="50dip"
            android:layout_height="40dip"
            android:inputType="number"
            android:id="@+id/editText_shelf"
            android:nextFocusDown="@+id/editText_request"
            android:nextFocusLeft="@+id/editText_request"
            android:nextFocusRight="@+id/editText_request"
            android:nextFocusUp="@+id/editText_request"
            android:nextFocusForward="@+id/editText_request"
            android:textColor="#ffffff"
            android:background="@drawable/text_box_background"
            android:focusable="true"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView20"
            android:layout_toEndOf="@+id/textView20"
            android:layout_marginLeft="2dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Order Qty"
            android:textColor="#ffffff"
            android:id="@+id/textView23"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/editText_shelf"
            android:layout_toEndOf="@+id/editText_shelf"
            android:layout_marginLeft="5dp" />

        <EditText
            android:layout_width="50dip"
            android:layout_height="40dip"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText_request"
            android:focusable="true"
            android:textColor="#ffffff"
            android:background="@drawable/text_box_background"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView23"
            android:layout_toEndOf="@+id/textView23"
            android:layout_marginLeft="2dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Free"
            android:id="@+id/textView_free"
            android:textColor="#ffffff"
            android:visibility="visible"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/editText_request"
            android:layout_toEndOf="@+id/editText_request" />

        <EditText
            android:layout_width="50dip"
            android:layout_height="40dip"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText_free"
            android:textColor="#ffffff"
            android:background="@drawable/text_box_background"
            android:visibility="visible"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView_free"
            android:layout_toEndOf="@+id/textView_free"
            android:layout_marginLeft="2dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Selected Product :"
            android:id="@+id/textView45"
            android:textColor="#ffffff"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/editText_free"
            android:layout_toEndOf="@+id/editText_free" />

        <Button
            android:layout_width="100dip"
            android:layout_height="50dip"
            android:text="Done"
            android:id="@+id/button_done"
            android:background="@drawable/button_green"
            android:textColor="#ffffff"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="5dp" />

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/button_done"
            android:layout_toStartOf="@+id/button_done">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/relativeLayout31">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Total Qty : 0"
                    android:id="@+id/textView25"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_below="@+id/relativeLayout31"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Total Free : 0"
                    android:id="@+id/textView26"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Rem. Free Qty : 0"
                    android:id="@+id/textView27"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayoutquentity"
        android:layout_alignLeft="@+id/relativeLayoutquentity"
        android:layout_alignStart="@+id/relativeLayoutquentity"
        android:background="#292323">

    </RelativeLayout>


</RelativeLayout>